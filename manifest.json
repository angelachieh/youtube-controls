{
	"name": "Youtube Controls",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "This extension allows you to control (fast forward, rewind, go to a specific time, set playback rate) Youtube or any other HTML5 videos.",
	"homepage_url": "http://extensionizr.com",
	"icons": {
		"16": "img/icon_16.png",
		"48": "img/icon_48.png",
		"128": "img/icon_128.png"
	},
	"background": {
		"scripts": ["js/background.js"],
		"persistent": true
	},
	"browser_action": {
		"default_icon": "img/icon_19.png",
		"default_title": "Youtube Controls",
		"default_popup": "views/popup.html"
	},
	"permissions": [
	],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["js/content.js"],
			"all_frames": true
		}
	]
}